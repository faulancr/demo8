{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms": "^8.7",
    "mask/mask": "^3.0",
    "helhum/typo3-console": "^4.5",
    "lochmueller/staticfilecache": "^4.0",
    "dmitryd/typo3-realurl": "^2.2",
    "jlawrence/eos": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Faulancr\\DdBoilerplate\\": "Web/typo3conf/ext/ddboilerplate/Classes/",
      "In2code\\Lux\\": "Web/typo3conf/ext/lux/Classes"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "Web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    }
  }
}
